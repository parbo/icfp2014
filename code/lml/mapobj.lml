(letrec (mapobj findx)
        ((lambda (map x y)
            (if (eq (print y) 0)
                (findx (car map) x)
                (mapobj (cdr map) x (- y 1))))
         (lambda (row x)
             (if (eq (print x) 0)
              	(car row)
                (findx (cdr row) (- x 1)))))
  (mapobj (list (list 0 1 2 3)
                (list 10 11 12 13)
                (list 20 21 22 23)) 2 2)
)
